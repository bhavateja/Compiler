decl
	int arr[20];
	int n,k;
	int b,c,d;
    int sample(int x, int y, int z),fact(int x);
enddecl

int sample(int x, int y, int z){
    decl
        int a;
    enddecl
    a = x*y;
    write(a);
    return a;
}

int fact(int x){
    decl
        int a;
    enddecl

    if(x == 1) then
        a = 1;
    else
        a = x*fact(x-1);
    endif;
    return a;
}

int main(){
    decl
        int ab,bc;
    enddecl
    write(sample(sample(23,12,22),2,3));
    write(fact(10));
    return 1;
}
